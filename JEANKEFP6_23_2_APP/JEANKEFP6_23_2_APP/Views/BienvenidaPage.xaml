<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="JEANKEFP6_23_2_APP.Views.BienvenidaPage" BackgroundImageSource="BienvenidaIcon">
    <ContentPage.Content>
        <StackLayout VerticalOptions="Center" Padding="50, 25, 50, 25">
            
            <Button x:Name="BtnLogin" Text="Ingresar" BackgroundColor="Green" CornerRadius="15" Clicked="BtnLogin_Clicked" />
            <Button x:Name="BtnAsks" Text="Ver todas las preguntas" BackgroundColor="Cyan" CornerRadius="15" Clicked="BtnAsks_Clicked" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>